{
  "files.exclude": {
    "reports/": true,
    "out/": false,
    ".vs/": true
  },
  "search.exclude": {
    "out/": true
  },
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "[yaml]": {
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "files.associations": {
    "**/.azure-pipelines/**/*.yaml": "azure-pipelines"
  },
  "cSpell.words": [
    "cmdlets",
    "Sarif"
  ],
  "git.branchProtection": [
    "main",
    "release/*"
  ]
}
